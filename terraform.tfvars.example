# Copy to terraform.tfvars and maintain values here.

cluster_name = "my-eks-cluster"
aws_region   = "us-west-2"

environment = "dev"

kubernetes_version = "1.33"

vpc_cidr = "10.0.0.0/16"

# One CIDR per AZ (match number of AZs or leave default)
private_subnet_cidrs = ["10.0.1.0/24", "10.0.2.0/24"]
public_subnet_cidrs  = ["10.0.101.0/24", "10.0.102.0/24"]

# Optional: override AZs (e.g. ["us-west-2a", "us-west-2b"])
# availability_zones = null

eks_managed_node_instance_types = ["t3.medium"]
eks_managed_node_desired_size   = 1
eks_managed_node_min_size       = 1
eks_managed_node_max_size       = 3
